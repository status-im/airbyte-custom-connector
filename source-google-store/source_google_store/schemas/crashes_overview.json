{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Date": {
      "type": "string",
      "description": "The date of the crash data"
    },
    "Package Name": {
      "type": "string",
      "description": "The package name of the app"
    },
    "Daily Crashes": {
      "type": "number",
      "description": "Number of crashes"
    },
    "Daily ANRs": {
      "type": "number",
      "description": "Number of Application Not Responding events"
    },
    "_metadata": {
      "type": "object",
      "properties": {
        "package_name": {
          "type": "string"
        },
        "report_path": {
          "type": "string"
        },
        "extracted_at": {
          "type": "string"
        },
        "stats_type": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "Date",
    "Package Name",
    "_metadata"
  ]
}
