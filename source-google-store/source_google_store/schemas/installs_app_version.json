{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Date": {
      "type": "string",
      "description": "The date of the installs data"
    },
    "Package name": {
      "type": "string",
      "description": "The package name of the app"
    },
    "App Version Code": {
      "type": "string",
      "description": "The version of the app"
    },
    "Daily Device Installs": {
      "type": "number",
      "description": "Number of daily device installs"
    },
    "Daily Device Uninstalls": {
      "type": "number",
      "description": "Number of daily device uninstalls"
    },
    "Daily Device Upgrades": {
      "type": "number",
      "description": "Number of daily device upgrades"
    },
    "Total User Installs": {
      "type": "number",
      "description": "Total number of user installs"
    },
    "Daily User Installs": {
      "type": "number",
      "description": "Number of daily user installs"
    },
    "Daily User Uninstalls": {
      "type": "number",
      "description": "Number of daily user uninstalls"
    },
    "Active Device Installs": {
      "type": "number",
      "description": "Number of active device installs"
    },
    "Install events": {
      "type": "number",
      "description": "Number of install events"
    },
    "Update events": {
      "type": "number",
      "description": "Number of update events"
    },
    "Uninstall events": {
      "type": "number",
      "description": "Number of uninstall events"
    },
    "_metadata": {
      "type": "object",
      "properties": {
        "package_name": {
          "type": "string",
          "description": "The package name of the app"
        },
        "report_path": {
          "type": "string",
          "description": "The path to the report file"
        },
        "extracted_at": {
          "type": "string",
          "description": "Timestamp when the data was extracted"
        },
        "stats_type": {
          "type": "string",
          "description": "Type of stats (app_version)"
        }
      },
      "required": ["package_name", "report_path", "extracted_at", "stats_type"]
    }
  },
  "required": ["Date", "Package Name", "App Version", "_metadata"]
} 