{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": [ "null", "string" ]
    },
    "name": {
      "type": [ "null", "string" ]
    },
    "username": {
        "type": [ "null", "string" ]
    },
    "description": {
        "type": [ "null", "string" ]
    },
    "most_recent_tweet_id": {
        "type": [ "null", "string" ]
    },
    "pinned_tweet_id": {
        "type": [ "null", "string" ]
    },
    "protected": {
        "type": [ "null", "boolean" ]
    },
    "verified_type": {
        "type": [ "null", "string" ]
    },
    "created_at": {
        "type": [ "null", "string" ],
        "format": "date-time"
    },
    "public_metrics": {
      "type": ["null", "object" ],
      "properties": {
        "tweet_count": {
          "type": [ "null", "number" ]
        },
        "like_count": {
          "type": [ "null", "number" ]
        },
        "following_count": {
          "type": [ "null", "number" ]
        },
        "followers_count": {
          "type": [ "null", "number" ]
        },
        "listed_count": {
          "type": [ "null", "number" ]
        }
      }
    }
  }
}
